<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_splash">
    <fragment
        android:id="@+id/navigation_splash"
        android:name="oaemdl.es.apkcavoshcafe.ui.Splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_navigation_splash_to_navigation_login"
            app:destination="@id/navigation_login" />
    </fragment>
    <fragment
        android:id="@+id/navigation_login"
        android:name="oaemdl.es.apkcavoshcafe.ui.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_navigation_login_to_navigation_registrar"
            app:destination="@id/navigation_registrar" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_inicio"
            app:destination="@id/navigation_inicio" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_passwordd"
            app:destination="@id/navigation_passwordd" />
    </fragment>
    <fragment
        android:id="@+id/navigation_registrar"
        android:name="oaemdl.es.apkcavoshcafe.ui.Registrar"
        android:label="fragment_registrar"
        tools:layout="@layout/fragment_registrar" >
        <action
            android:id="@+id/action_navigation_registrar_to_navigation_inicio"
            app:destination="@id/navigation_inicio" />
        <action
            android:id="@+id/action_navigation_registrar_to_navigation_login"
            app:destination="@id/navigation_login" />
    </fragment>
    <fragment
        android:id="@+id/navigation_inicio"
        android:name="oaemdl.es.apkcavoshcafe.ui.Inicio"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_menu"
            app:destination="@id/navigation_menu" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_favoritos"
            app:destination="@id/navigation_favoritos" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_carrito"
            app:destination="@id/navigation_carrito" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_perfil"
            app:destination="@id/navigation_perfil" />
    </fragment>
    <fragment
        android:id="@+id/navigation_menu"
        android:name="oaemdl.es.apkcavoshcafe.ui.Menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
    <fragment
        android:id="@+id/navigation_favoritos"
        android:name="oaemdl.es.apkcavoshcafe.ui.Favoritos"
        android:label="fragment_favoritos"
        tools:layout="@layout/fragment_favoritos" />
    <fragment
        android:id="@+id/navigation_carrito"
        android:name="oaemdl.es.apkcavoshcafe.ui.Carrito"
        android:label="fragment_carrito"
        tools:layout="@layout/fragment_carrito" />
    <fragment
        android:id="@+id/navigation_perfil"
        android:name="oaemdl.es.apkcavoshcafe.ui.Perfil"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/navigation_passwordd"
        android:name="oaemdl.es.apkcavoshcafe.ui.Passwordd"
        android:label="fragment_passwordd"
        tools:layout="@layout/fragment_passwordd" />
</navigation>